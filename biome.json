{
  "$schema": "https://biomejs.dev/schemas/1.2.2/schema.json",
  "organizeImports": {
    "enabled": true
  },
  "linter": {
    "enabled": true,
    "rules": {
      "recommended": true,
      "style": {
        "useConst": {
          "level": "off"
        },
        "noUselessElse": {
          "level": "off"
        }
      },
      "complexity": {
        "noForEach": {
          "level": "off"
        }
      }
    }
  }
}
